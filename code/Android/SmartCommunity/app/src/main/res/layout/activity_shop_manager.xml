<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="com.project.community.ui.me.shop_management.ShopManagerActivity">

    <include layout="@layout/pub_layout_toolbar" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:paddingBottom="39dp"
                android:paddingTop="39dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/shop_manager_liushui"
                        android:textSize="18sp"
                        android:textColor="@color/colorPrimary"
                        android:text="290/16564"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:textSize="12sp"
                        android:layout_marginTop="18dp"
                        android:textColor="@color/color_gray_999999"
                        android:text="@string/shop_manager_liushui"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/color_gray_e5e5e5"/>
                <LinearLayout
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/shop_manager_order"
                        android:textSize="18sp"
                        android:textColor="@color/colorPrimary"
                        android:text="290/16564"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:textSize="12sp"
                        android:layout_marginTop="18dp"
                        android:textColor="@color/color_gray_999999"
                        android:text="@string/shop_manager_order"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_10"
                android:background="@color/color_gray_eeeeee" />

            <LinearLayout
                android:paddingBottom="72dp"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:paddingTop="47dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/shop_manager_goods_manager"
                    android:paddingTop="@dimen/dp_5"
                    android:drawableTop="@mipmap/d75_icon1"
                    android:text="@string/shop_manager_goods"
                    style="@style/shop_manager_tv_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/shop_manager_data_manager"
                    android:paddingTop="@dimen/dp_5"
                    android:drawableTop="@mipmap/d75_icon2"
                    android:text="@string/shop_manager_data"
                    style="@style/shop_manager_tv_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <FrameLayout
                    android:id="@+id/shop_manager_order_manager"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:paddingTop="@dimen/dp_5"
                        android:drawableTop="@mipmap/d75_icon3"
                        android:text="@string/shop_manager_order_manager"
                        style="@style/shop_manager_tv_title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/shop_manager_order_num"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_gravity="center_horizontal"
                        android:text="1"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textSize="10sp"
                        android:background="@mipmap/d75_dian"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </FrameLayout>

                <TextView
                    android:id="@+id/shop_manager_zhanghao_manager"
                    android:paddingTop="@dimen/dp_5"
                    android:drawableTop="@mipmap/d75_icon4"
                    android:text="@string/shop_manager_zhanghao_manager"
                    style="@style/shop_manager_tv_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_gray_eeeeee" />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:background="@color/white">

                <TextView
                    android:id="@+id/tv_push"
                    style="@style/textview_style"
                    android:textSize="14sp"
                    android:layout_height="match_parent"
                    android:paddingLeft="@dimen/dp_15"
                    android:text="@string/shop_manager_shop_yingye"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <com.alanapi.switchbutton.SwitchButton
                    android:id="@+id/switchButton"
                    style="@style/sw_SwitchButtonIOS"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="15dp"
                    android:checked="true"
                    android:gravity="center_vertical" />
                <!-- <com.suke.widget.SwitchButton
                     android:id="@+id/switchButton"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     app:sb_checked="true"
                     android:visibility="gone"
                     android:layout_centerInParent="true"
                     android:layout_alignParentRight="true"
                     android:layout_marginRight="30dp"
                     android:layout_centerVertical="true"/>-->
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/color_gray_eeeeee" />
        </LinearLayout>
    </ScrollView>



</LinearLayout>
